<div id="iconFlex" class="iconFlex">
  <div class="columnFlex">
      <a class="noDec" href="/">
        <img class="mainIcon" src="../public/watericon.png">
        <div class="mainText">chute</div>    
      </a>
  </div>
</div>
<div class="logFlex">
    <div class='loginCont'>
    <div class="Login">Register</div>
    <% include ./partials/messages %>
  <form class='loginForm' action='/users/Register' method='POST'>
    <div class='inputFlex'>
      <div class='formText'>Username</div>
      <input type='name' id='name' name='name' placeholder="Enter Username.." value="<%= typeof name != 'undefined' ? name : '' %>" onkeypress="return noSpace(event)">    
    </div>
    <div class='inputFlex'>
      <div class='formText'>Email</div>
      <input type='email' id='email' name='email' placeholder="Enter Email.." value="<%= typeof email != 'undefined' ? email : '' %>" onkeypress="return noSpace(event)">
    </div>
    <div class='inputFlex'>
      <div class='formText'>Password</div>
      <input type='password' id='password' name='password' placeholder="Create Password.." value="<%= typeof password != 'undefined' ? password : '' %>" onkeypress="return noSpace(event)">    
    </div>
    <div class='inputFlex'>
      <div class='formText'>Confirm Password</div>
      <input type='password' id='password2' name='password2' placeholder="Confirm Password..." value="<%= typeof password2 != 'undefined' ? password2 : '' %>" onkeypress="return noSpace(event)">    
    </div>
    <button type='submit' class='Submit'>Register</button>
  </form>
  <div class="noAccount">
    Have an account?
    <a href="/users/login">Login</a>
  </div>
</div>
</div>
